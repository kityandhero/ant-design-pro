{
  "name": "ant-design-pro",
  "version": "2.0.0-beta.1",
  "description": "An out-of-box UI solution for enterprise applications",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "start": "cross-env ESLINT=none PORT=9901 roadhog dev",
    "start:no-proxy": "cross-env NO_PROXY=true ESLINT=none port=9901 roadhog dev",
    "build": "cross-env ESLINT=none roadhog build",
    "site": "roadhog-api-doc static && gh-pages -d dist",
    "analyze": "cross-env ANALYZE=true roadhog build",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint": "eslint --ext .js src mock tests && npm run lint:style",
    "lint:fix": "eslint --fix --ext .js src mock tests && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "test": "roadhog test",
    "test:component": "roadhog test ./src/components",
    "test:all": "node ./tests/run-tests.js",
    "prettier": "prettier --write ./src/**/**/**/*"
  },
  "dependencies": {
    "@antv/data-set": "^0.10.1",
    "@babel/polyfill": "^7.0.0",
    "antd": "^3.10.8",
    "bizcharts": "^3.3.1",
    "bizcharts-plugin-slider": "^2.1.1-beta.1",
    "braft-editor": "^2.1.30",
    "classnames": "^2.2.6",
    "dva": "^2.4.1",
    "dva-loading": "^2.0.6",
    "enquire-js": "^0.2.1",
    "lodash": "^4.17.11",
    "lodash-decorators": "^6.0.0",
    "moment": "^2.22.2",
    "numeral": "^2.0.6",
    "omit.js": "^1.0.0",
    "path-to-regexp": "^2.4.0",
    "prop-types": "^15.6.2",
    "qs": "^6.5.2",
    "rc-drawer": "^1.7.6",
    "react": "^16.6.3",
    "react-container-query": "^0.11.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.6.3",
    "react-fittext": "^1.0.0",
    "react-syntax-highlighter": "^10.1.1",
    "rollbar": "^2.5.0",
    "setprototypeof": "^1.1.0",
    "url-polyfill": "^1.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "babel-eslint": "^10.0.1",
    "babel-plugin-dva-hmr": "^0.4.1",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "cross-env": "^5.2.0",
    "cross-port-killer": "^1.0.1",
    "enzyme": "^3.7.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^2.6.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-markdown": "^1.0.0-rc.0",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.0.1",
    "husky": "^1.2.0",
    "lint-staged": "^8.0.5",
    "mockjs": "^1.0.1-beta3",
    "prettier": "1.15.2",
    "pro-download": "^1.0.1",
    "react-loadable": "^5.5.0",
    "redbox-react": "^1.6.0",
    "regenerator-runtime": "^0.13.1",
    "roadhog": "^2.5.0-beta.2",
    "roadhog-api-doc": "^1.1.2",
    "stylelint": "^9.8.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "optionalDependencies": {
    "puppeteer": "^1.10.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,less}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "eslint --ext .js",
    "**/*.less": "stylelint --syntax less"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
